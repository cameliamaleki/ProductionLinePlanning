@using Core;
@model Sell.LendingCars.LendingCarsPrintModel


<style>
    .wwc {
        min-width: 100px !important;
    }
</style>
<script type="text/javascript">
    $(document).ready(function (e) {
        $("#LCP_IDrequstAmani").ajaxChosen({
            dataType: 'json',
            type: 'POST',
            url: '/LendingCars/searchLendingCars'
        }, {
            processItems: function (data) {
                console.log(data);
                return data.results;
            },
            minLength: 2
        });

        $("#LCP_F_AgnID").ajaxChosen({
            dataType: 'json',
            type: 'POST',
            url: '/Contract/searchAgents'
        }, {
            processItems: function (data) {
                console.log(data);
                return data.results;
            },
            minLength: 2
        });
    });

</script>
@using (Html.BeginForm("LendingCarsPrint", "LendingCars", FormMethod.Post, new { id = "frmlendingcarsprint", @class = "form-horizontal" }))
{



    <div class="row-fluid" style="float: right; margin-bottom: 20px;">
        <div class="span4" style="float: right">
            <div class="form-horizontal" style="margin-left: 30px;">
                <div class="user-line">
                    <div class="slabel">

                        @Html.LabelFor(model => model.LCP_FromDate)
                    </div>

                    @Html.EditorFor(model => model.LCP_FromDate)
                    @Html.ValidationMessageFor(model => model.LCP_FromDate)
                </div>
            </div>
        </div>

        <div class="span4" style="float: right">
            <div class="form-horizontal" style="margin-left: 30px;">
                <div class="user-line">
                    <div class="slabel">
                        @Html.LabelFor(model => model.LCP_ToDate)
                    </div>

                    @Html.EditorFor(model => model.LCP_ToDate)
                    @Html.ValidationMessageFor(model => model.LCP_ToDate)
                </div>
            </div>
        </div>
    </div>

    <div class="row-fluid" style="float: right; margin-bottom: 20px;">
        <div class="span8" style="float: right">
            <div class="form-horizontal" style="margin-left: 30px;">
                <div class="user-line">
                    <div class="slabel">
                        @Html.LabelFor(model => model.LCP_F_AgnID)
                    </div>

                    @Html.DropDownListFor(model => model.LCP_F_AgnID, new List<SelectListItem>(), new { @class = "span10 ", data_placeHolder = "انتخاب کنید" })
                    @Html.ValidationMessageFor(model => model.LCP_F_AgnID)
                </div>

                <div class="user-line">
                    <div class="slabel">
                        @Html.LabelFor(model => model.LCP_IDrequstAmani)
                    </div>

                    @Html.DropDownListFor(model => model.LCP_IDrequstAmani, new List<SelectListItem>(), new { @class = "span10  ", data_placeHolder = "انتخاب کنید" })
                    @Html.ValidationMessageFor(model => model.LCP_IDrequstAmani)
                </div>
                <div class="user-line">
                    <div class="slabel">
                        @Html.LabelFor(model => model.LCP_code)
                    </div>

                    @Html.DropDownListFor(model => model.LCP_code, new List<SelectListItem>(), new { @class = "span10 wwc  ", data_placeHolder = "انتخاب کنید" })
                    @Html.ValidationMessageFor(model => model.LCP_code)
                </div>



                <div class="user-line">
                    <div class="slabel">
                        @Html.LabelFor(model => model.LCP_TrackID)
                    </div>

                    @Html.DropDownListFor(model => model.LCP_TrackID, new List<SelectListItem>(), new { @class = "span10 wwc chosen ", data_placeHolder = "انتخاب کنید" })
                    @Html.ValidationMessageFor(model => model.LCP_TrackID)
                </div>

            </div>
        </div>
    </div>
}

<script type="text/javascript">

    $(function (e) {

        $('#LCP_FromDate , #LCP_ToDate').datepicker({
            dateFormat: 'yy/mm/dd'
        }).mask("9999/99/99");
        SetLayout();
    });

</script>


